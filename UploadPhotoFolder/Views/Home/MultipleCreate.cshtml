@model MultiplePhotoUploadViewModel
@{
    ViewData["Title"] = "MultipleCreate";
}

<h1>MultipleCreate</h1>

<form method="post" enctype="multipart/form-data">
    <div class="col-md-12">
        <div asp-validation-summary="All" class="text-danger">
        </div>
        <div class="form-group">
            <label asp-for="Photos"></label>
            <input asp-for="Photos" class="form-control" multiple />
            <span asp-validation-for="Photos" class="text-danger"></span>
        </div>

        <button type="submit" class="btn btn-primary">Upload it </button>
    </div>

</form>